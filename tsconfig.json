{
  "compilerOptions": {
    // ... other options
    "jsx": "react-native",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".", // Add this line if it's missing
    "paths": {
      "*": ["./src/*"] // Add this line to help resolve modules
    }
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*", "App.tsx"] // <---  Include your source files, and App.tsx
}
